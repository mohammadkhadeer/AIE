<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">


    <item
        android:state_pressed="true"
        >
        <shape
            android:shape="rectangle">

            <stroke
                android:width="0.2dp"
                android:color="#000" >
            </stroke>


        </shape>

    </item><!-- pressed -->
    <item
        android:state_focused="true"
        >
        <shape
            android:shape="rectangle">

            <stroke
                android:width="0.2dp"
                android:color="#000" >
            </stroke>


        </shape>
    </item><!-- focused -->
    <item
        >
        <shape
            android:shape="rectangle">

            <stroke
                android:width="0.2dp"
                android:color="#000" >
            </stroke>


        </shape>
    </item><!-- default -->


</selector>